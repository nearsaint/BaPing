	@charset "UTF-8";
	body {
		background: #000;
	}
	
	body > section.p1,
	body > section.p2,
	body > section.p3,
	body > section.p4,
	body > section.p5 {
		width: 100%;
		height: 100%;
		padding: 100px 15px;
		position: relative;
	}
	
	body > .title {
		position: absolute;
		top: 15px;
		left: 15px;
		width: calc(100% - 30px);
		height: 60px;
		font-size: 20px;
		z-index: 99;
		line-height: 60px;
	}
	
	body > .title > .name {
		position: absolute;
		top: 0;
		left: 15px;
		font-size: 20px;
		color: rgb(255, 255, 255);
	}
	
	body > .title > .name > i {
		font-size: 16px;
		font-style: normal;
	}
	
	body > .title > .copyright {
		position: absolute;
		top: 0;
		right: 15px;
		font-size: 12px;
		font-weight: 100;
		color: #999;
	}
	
	body > .video {
		position: absolute;
		z-index: -1;
		width: 100%;
		height: 100%;
		top: 0;
		left: 0;
		right: 0;
		text-align: center;
		vertical-align: middle;
		overflow: hidden;
	}
	
	body > .video > video {
		position: absolute;
		top: 50%;
		left: 50%;
		width: 100%;
		height: auto;
		margin-left: -50%;
		margin-top: -50%;
		z-index: 2;
	}
	
	body > div.tools {
		position: absolute;
		bottom: 15px;
		right: 15px;
		height: 80px;
		width: calc(100% - 30px);
		z-index: 99;
		overflow: hidden;
	}
	
	body > div.tools > h5.openLink {
		position: absolute;
		right: 0;
		bottom: 0;
		height: 60px;
		width: 60px;
		margin: 10px 10px;
		color: #fff;
		font-size: 32px;
		line-height: 60px;
		text-align: center;
		display: inline-block;
		cursor: pointer;
		transition: all 0.2s;
		border-radius: 5px;
	}
	
	body > div.tools > h5.openLink:hover {
		background: rgba(255, 255, 255, 0.9);
		color: #333;
	}
	
	body > div.tools > ul.link {
		position: absolute;
		right: -100%;
		bottom: 0;
		height: 80px;
		background: rgba(255, 255, 255, 0.9);
		overflow: hidden;
		border-radius: 5px;
	}
	
	body > div.tools > ul.link > li {
		width: 60px;
		height: 60px;
		margin: 10px;
		float: left;
	}
	
	body > div.tools > ul.link > li > a {
		display: block;
		width: 100%;
		height: 100%;
		text-align: center;
		cursor: pointer;
		font-size: 12px;
		color: #333;
	}
	
	body > div.tools > ul.link > li > a > span {
		display: block;
		height: 37px;
		width: 37px;
		margin: 5px auto;
		background-image: url(../images/daping_logo.png);
		background-position: center;
		background-size: contain;
		background-repeat: no-repeat;
	}
	
	body > div.tools > ul.link > li > a.closeLink {
		font-size: 28px;
		color: #333;
		line-height: 60px;
	}
	/*******************************************
*
*  section p1
*
*******************************************/
	
	section.p4 > .videoMask,
	section.p3 > .videoMask,
	section.p1 > .videoMask {
		width: 100%;
		height: 100%;
		display: block;
		background: rgba(0, 0, 0, 0.9);
		position: absolute;
		top: 0;
		left: 0;
		z-index: 1;
	}
	
	section.p1 > div.userJoin {
		position: absolute;
		top: 50%;
		left: 15px;
		max-height: 450px;
		width: 360px;
		overflow: hidden;
		z-index: 4;
		transform: translateY(-50%);
	}
	
	section.p1 > div.userJoin > ul {
		height: auto;
		width: 100%;
	}
	
	section.p1 > div.userJoin > ul > li {
		height: 70px;
		width: 100%;
		margin-bottom: 20px;
	}
	
	section.p1 > div.userJoin > ul > li > img {
		background: #ccc;
		border-radius: 50px;
		height: 70px;
		width: 70px;
		border: 3px solid #fff;
		display: inline-block;
		float: left;
	}
	
	section.p1 > div.userJoin > ul > li > img[sex='1'] {
		border-color: #00a0e9;
	}
	
	section.p1 > div.userJoin > ul > li > img[sex='2'] {
		border-color: #f867d1;
	}
	
	section.p1 > div.userJoin > ul > li > h5 {
		margin-left: 80px;
		font-size: 24px;
		color: #fff;
		line-height: 70px;
	}
	
	section.p1 > div.userJoin > ul > li > h5 > span {
		font-weight: bold;
		margin-right: 10px;
		display: inline-block;
		max-width: 120px;
		overflow: hidden;
		text-overflow: ellipsis;
		white-space: nowrap;
		float: left;
	}
	
	section.p1 > div.userJoin > ul > li > h5 > b {
		display: inline-block;
		font-weight: 100;
		font-size: 20px;
	}
	
	section.p1 > div.title {
		width: 620px;
		height: 500px;
		position: absolute;
		left: 18%;
		top: 50%;
		transform: translateY(-55%);
		z-index: 3;
	}
	
	section.p1 > div.title > div.gameName {
		height: 420px;
		width: 100%;
		background-image: url(http://balabalafun.b0.upaiyun.com/game/game_02/server/gameName.png);
		background-position: top center;
		background-repeat: no-repeat;
		background-size: contain;
	}
	
	section.p1 > div.title > h5 {
		text-align: center;
		font-size: 38px;
		font-weight: bold;
		color: #fff;
		transform: rotate(0) skew(-16deg);
		-webkit-transform: rotate(0) skew(-16deg);
	}
	
	section.p1 > div.title > h5 > b {
		margin: 0 15px;
		font-size: 50px;
	}
	
	section.p1 > div.qrCode {
		width: 300px;
		position: absolute;
		left: 50%;
		top: 50%;
		transform: translateY(-50%) translateX(320px);
		z-index: 3;
	}
	
	section.p1 > div.maxQrCode > a.closeMaxQrCode,
	section.p1 > div.qrCode > a.openMaxQrCode {
		position: absolute;
		top: -16px;
		right: -16px;
		width: 36px;
		height: 36px;
		display: block;
		background: #333;
		color: #fff;
		border-radius: 50px;
		font-size: 20px;
		line-height: 34px;
		text-align: center;
		cursor: pointer;
	}
	
	section.p1 > div.maxQrCode > a.closeMaxQrCode:hover,
	section.p1 > div.qrCode > a.openMaxQrCode:hover {
		color: #fff;
		background: #ccc;
	}
	
	section.p1 > div.qrCode > span {
		background: #fff;
		border-radius: 10px 10px 0 0;
		display: block;
		height: 300px;
		width: 300px;
		padding: 5px;
		background-repeat: no-repeat;
		background-size: contain;
	}
	
	section.p1 > div.qrCode > h5 {
		background: #fff;
		border-radius: 0 0 10px 10px;
		display: block;
		height: 50px;
		width: 100%;
		font-size: 22px;
		font-weight: bold;
		color: #333;
		line-height: 39px;
		text-align: center;
		padding-bottom: 10px;
		margin-top: -2px;
		border-bottom: 2px solid #ccc;
	}
	
	section.p1 > div.qrCode > div.openGame {
		margin-top: 20px;
		width: 100%;
		height: 80px;
	}
	
	section.p1 > div.qrCode > div.openGame > a {
		display: block;
		width: 100%;
		height: 100%;
		border-radius: 10px;
		color: #333;
		line-height: 50px;
		font-size: 24px;
		background: #fff;
		border-bottom: 5px solid #ccc;
		transition: border 0.3s;
		text-align: center;
	}
	
	section.p1 > div.qrCode > div.openGame > a >span {
		display: block;
		font-size: 14px;
		line-height: 10px;
	}
	
	section.p1 > div.qrCode > div.openGame > a:hover {
		border-bottom: 8px solid #ccc;
	}
	
	section.p1 > div.qrCode > div.openGame > a:focus,
	section.p1 > div.qrCode > div.openGame > a:active {
		border-bottom: 3px solid #ccc;
	}
	
	section.p1 > div.maxQrCode {
		position: absolute;
		top: 50%;
		left: 50%;
		transform: translateY(-50%) translateX(-50%);
		height: 700px;
		width: 600px;
		background: #fff;
		border-radius: 10px;
		padding: 5px;
		display: none;
		z-index: 4;
	}
	
	section.p1 > div.maxQrCode > a.closeMaxQrCode:hover {
		color: #fff;
		background: #ccc;
	}
	
	section.p1 > div.maxQrCode > span {
		height: 590px;
		width: 590px;
		display: block;
		background-repeat: no-repeat;
		background-size: contain;
	}
	
	section.p1 > div.maxQrCode > h5 {
		font-size: 28px;
		margin: 5px 0 20px;
		text-align: center;
	}
	
	section.p1 > div.maxQrCode > h5 > b {
		margin: 0 10px;
		font-size: 32px;
	}
